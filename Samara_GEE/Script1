var paises = ee.FeatureCollection("USDOS/LSIB_SIMPLE/2017")
print(paises.size())
paises = paises.filterMetadata('country_co','equals','BR')
print(paises.size())
Map.addLayer(paises)
var lista = ee.List([])
var N = 3
for(var i =0; i < N; i++) {
  var imgRand = ee.Image.random(i)
  lista = lista.add(imgRand)
}
var imgCol = ee.ImageCollection(lista)
imgCol = imgCol.filterBounds(paises)
Map.addLayer(imgCol.median())
Map.addLayer(imgCol.min())
Map.addLayer(imgCol.max())

var imageHist = imgCol.median()
var histogram = ui.Chart.image.histogram({
  image:imageHist,
  region:paises,
  scale: 3000,
  minBucketWidth: 0.01
})
print(histogram)
